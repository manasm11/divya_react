{
    "name": "react-notes",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=14.9.0"
    },
    "license": "MIT",
    "dependencies": {
        "@babel/core": "7.12.3",
        "@babel/register": "^7.12.1",
        "babel-loader": "8.1.0",
        "babel-preset-react-app": "10.0.0",
        "concurrently": "^5.3.0",
        "excerpts": "^0.0.3",
        "express": "^4.17.1",
        "html-webpack-plugin": "4.5.0",
        "nodemon": "^2.0.6",
        "react": "0.0.0-experimental-3310209d0",
        "react-dom": "0.0.0-experimental-3310209d0",
        "react-error-boundary": "^3.1.0",
        "react-fetch": "0.0.9",
        "react-server-dom-webpack": "0.0.0-experimental-3310209d0",
        "resolve": "1.12.0",
        "rimraf": "^3.0.2",
        "webpack": "4.44.2",
        "webpack-cli": "^4.2.0"
    },
    "devDependencies": {
        "prettier": "1.19.1"
    },
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run bundler\"",
        "start-dev": "npm run bundler",
        "server": "NODE_ENV=development nodemon -- --conditions=react-server server/api.server.js",
        "bundler": "NODE_ENV=development nodemon -- scripts/build.js",
        "prettier": "prettier --write **/*.js"
    },
    "babel": {
        "presets": [
            [
                "react-app",
                {
                    "runtime": "automatic"
                }
            ]
        ]
    },
    "nodemonConfig": {
        "ignore": [
            "build/*"
        ]
    }
}
